// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
}

model Cap {
  id     Int    @id @default(autoincrement())
  userId Int    @unique
  data   String
  hash   Int
  clock  Int
}
