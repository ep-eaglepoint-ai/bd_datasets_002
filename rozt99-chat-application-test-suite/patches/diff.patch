+      fireEvent.click(screen.getByRole("button", { name: /send/i }));
+
+      act(() => {
+        jest.advanceTimersByTime(2500);
+      });
+      await waitFor(() =>
+        expect(screen.queryByText(/First Message/)).toBeInTheDocument(),
+      );
+    });
+  });
+});
