+        setResume({
+            ...resume,
+            [section]: (resume[section] as any[]).map((item: any) =>
+                item.id === id ? { ...item, [field]: value } : item
+            )
+        });
+    };
+
+    const renderSectionContent = (type: string) => {
+        switch (type) {
+            case 'personal':
+                return (
+                    <div className="grid grid-cols-1 md:grid-cols-2 gap-4">
+                        <Input label="Full Name" value={resume.personalInfo.fullName} onChange={e => updatePersonalInfo('fullName', e.target.value)} placeholder="John Doe" required />
+                        <Input label="Email" type="email" value={resume.personalInfo.email} onChange={e => updatePersonalInfo('email', e.target.value)} placeholder="john@example.com" required />
