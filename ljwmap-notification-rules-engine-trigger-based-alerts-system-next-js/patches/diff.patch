+  onChange: (enabled: boolean) => void;
+  label?: string;
+  disabled?: boolean;
+}
+
+export function Toggle({ enabled, onChange, label, disabled = false }: ToggleProps) {
+  return (
+    <label className="inline-flex items-center cursor-pointer">
+      <button
+        type="button"
+        role="switch"
+        aria-checked={enabled}
+        disabled={disabled}
